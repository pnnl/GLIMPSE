{
   "docs": "https://github.com/pnnl/GLIMPSE/tree/master/Docs",
   "appOptions": {
      "title": "GLIMPSE",
      "nav": {
         "home": "Home",
         "about": "About"
      },
      "buttons": {
         "filterBtn": "FILTER",
         "exportBtn": "EXPORT W/ CHANGES",
         "plotBtn": "SHOW PLOT",
         "addOverlayBtn": "ATTACH OVERLAY",
         "showStatsBtn": "SHOW STATS",
         "layoutLbl": "Auto Layout",
         "searchLbl": "ID Search",
         "previousBtn": "PREV",
         "nextBtn": "NEXT",
         "resetBtn": "RESET",
         "toggleLegendBtn": "TOGGLE LEGEND"
      }
   },
   "graphOptions": {
      "configure": {
         "enabled": true,
         "showButton": false
      },
      "edges": {
         "scaling": {
            "min": 5,
            "max": 20
         },
         "font": {
            "multi": true,
            "align": "horizontal",
            "strokeWidth": 3,
            "strokeColor": "#FFFFFF"
         },
         "smooth": {
            "enabled": false,
            "type": "continuous"
         }
      },
      "nodes": {
         "size": 15,
         "borderWidth": 2,
         "shapeProperties": {
            "interpolation": false
         },
         "font": {
            "color": "#000000",
            "multi": true,
            "align": "center",
            "bold": "15px arial black"
         },
         "scaling": {
            "min": 5,
            "max": 55
         }
      },
      "interaction": {
         "hover": true,
         "hideEdgesOnDrag": true,
         "hideEdgesOnZoom": true,
         "navigationButtons": true,
         "multiselect": true
      },
      "physics": {
         "enabled": true,
         "barnesHut": {
            "gravitationalConstant": -19500,
            "centralGravity": 1,
            "springConstant": 0.4,
            "springLength": 80,
            "avoidOverlap": 0.25,
            "damping": 0.15
         },
         "forceAtlas2Based": {
            "gravitationalConstant": -400,
            "centralGravity": 0.01,
            "springLength": 225,
            "springConstant": 0.1
         },
         "repulsion": {
            "centralGravity": 0.2,
            "springLength": 200,
            "springConstant": 0.05,
            "nodeDistance": 100,
            "damping": 0.09
