         "stabilization": {
            "enabled": true,
            "iterations": 1000,
            "updateInterval": 50,
            "onlyDynamicEdges": false,
            "fit": true
         },
         "solver": "barnesHut",
         "timestep": 0.5,
         "maxVelocity": 75,
         "minVelocity": 0.2,
         "adaptiveTimestep": true,
         "wind": {
            "x": 0,
            "y": 0
