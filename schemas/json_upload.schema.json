{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "JSON schema for validating json uploads",
  "type": "object",
  "properties": {
    "objects": {
      "type": "array",
      "items": {
        "type": "object",
        "patternProperties": {
          "name|objectType": { "type": "string" },
          "^elementType$": { "type": "string" },
          "^attributes$": {
            "type": "object",
            "patternProperties": {
              "name|id": { "type": "string" }
            },
            "additionalProperties": true
          }
        },
        "additionalProperties": false
      }
    }
  },
  "required": ["objects"],
  "additionalProperties": false
}
